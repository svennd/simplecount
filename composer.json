	"homepage": "https://github.com/svennd/simplecount",
	"version": "1.0.0",
	"time": "2015-8-17",
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "SvennD",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.3.3"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
